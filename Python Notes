import sys, csv

test_file = sys.argv[1]

delimiter= "\t"

highest = 0

with open(test_file, "r",) as source:
  row = csv.reader(source, delimiter="\t")
  next(row)

  for column1, column2, column3 in row:
    if int(column2) > highest:
      highest = column2
    else:
      pass
  print(highest)


