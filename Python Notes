import sys

test_file = sys.argv[1]

line_list = []
with open(test_file, "r") as source:
  line = source.readline()
  while line != "":
    line_list.append(line)
    line = source.readline()
  line_list.reverse()
  for i in line_list:
    print(i, end='')


